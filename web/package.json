{
  "private": true,
  "name": "recipes-sanity-nextjs",
  "version": "0.0.1",
  "author": "simeonGriggs <simeon@hey.com>",
  "scripts": {
    "dev": "next dev",
    "dev:network": "IP=$(ipconfig getifaddr en0); next dev -H $IP",
    "build": "next build",
    "sitemap": "node exportSitemap.js && next build",
    "format": "prettier-eslint --write \"**/*.js\" \"!.next/**\" \"!node_modules/**\" \"!out/**\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "next start"
  },
  "dependencies": {
    "@graywolfai/react-heroicons": "^1.2.3",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.1.0",
    "@sanity/image-url": "^1.0.1",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^10.1.0",
    "convert-units": "^2.3.4",
    "groq": "^2.0.9",
    "lodash.throttle": "^4.1.1",
    "next": "^12.0.8",
    "next-sanity": "^0.4.0",
    "next-seo": "^4.28.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-inlinesvg": "^2.2.2",
    "react-portal": "^4.2.1",
    "sitemap": "^7.1.0",
    "tailwindcss": "^3.0.13",
    "zustand": "^3.6.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 2,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 80,
          "semi": false
        }
      ]
    }
  }
}
